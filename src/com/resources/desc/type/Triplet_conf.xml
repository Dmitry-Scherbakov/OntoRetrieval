<?xml version="1.0" encoding="UTF-8"?>
<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
    <name>OntoRetrieval</name>
    <description/>
    <version>1.0</version>
    <types>
        <typeDescription>
            <name>com.ontological.retrieval.DataTypes.Triplet</name>
            <description>The fact structure. It is an analog of RDF triplet, but a little more.</description>
            <supertypeName>uima.tcas.Annotation</supertypeName>
            <features>
                <featureDescription>
                    <name>subject</name>
                    <description>Subject of a triplet.</description>
                    <rangeTypeName>com.ontological.retrieval.DataTypes.TripletField</rangeTypeName>
                </featureDescription>
                <featureDescription>
                    <name>object</name>
                    <description>Object of a triplet.</description>
                    <rangeTypeName>com.ontological.retrieval.DataTypes.TripletField</rangeTypeName>
                </featureDescription>
                <featureDescription>
                    <name>context</name>
                    <description>Context (sentence) of current triplet.</description>
                    <rangeTypeName>de.tudarmstadt.ukp.dkpro.core.api.segmentation.type.Sentence</rangeTypeName>
                </featureDescription>
                <featureDescription>
                    <name>relation</name>
                    <description>Relation of a triplet.</description>
                    <rangeTypeName>uima.cas.FSList</rangeTypeName>
                </featureDescription>
                <featureDescription>
                    <name>isRelation</name>
                    <description>Is relation of a triplet.</description>
                    <rangeTypeName>uima.cas.Boolean</rangeTypeName>
                </featureDescription>
                <featureDescription>
                    <name>isValid</name>
                    <description>Is valid of a triplet.</description>
                    <rangeTypeName>uima.cas.Boolean</rangeTypeName>
                </featureDescription>
                <featureDescription>
                    <name>clone</name>
                    <description>Clone a triplet.</description>
                    <rangeTypeName>com.ontological.retrieval.DataTypes.Triplet</rangeTypeName>
                </featureDescription>
                <featureDescription>
                    <name>score</name>
                    <description>Score of a triplet.</description>
                    <rangeTypeName>com.ontological.retrieval.DataTypes.TripletScore</rangeTypeName>
                </featureDescription>
            </features>
        </typeDescription>
    </types>
</typeSystemDescription>
